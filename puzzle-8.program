 function repeatDown(total) {
  var count = 0;
  while (count < total) {
    down();
    count = count + 1;
  }
   right();
   right();
}

function repeatUp(total) {
var count = 0;
   while (count < total) {
     up();
     count = count + 1;
   }
  right();
  right();
 
}
   
   
repeatDown(8) 
repeatUp(7)
repeatDown(7)
repeatUp(7)
repeatDown(7)
